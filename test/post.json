{
    "post":{
        "helpers": "",
        "partials": "",
        "template": "post.hbs",
        "data": {
            "article": {
                "filename": "article.md",
                "source": "{{read article.md}}",
                "title": "{{front-matter-extract source frontMatter.title}}",
                "author": "{{front-matter-extract source frontMatter.author}}",
                "content": "{{front-matter-extract source content}}",
                "url": "{{urlBuilder this.title}}",
                "image": "{{articleImgSrc this.title}}"
            },
            "rootFolder": "../../../"
        }
    }
}
